{
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "300mb" 
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    }
  ]
}